{
    "manifest_version": 2,
    "name": "Xbox Game DVR Grabber",
    "version": "0.1",
    "icons": { "64": "icon64.png" },
    "content_scripts": [
        {
            "matches": [
                "https://account.xbox.com/*/gameclip/*",
                "https://account.xbox.com/*/gameclips"
            ],
            "js": ["content.js"]
        }
    ]
}